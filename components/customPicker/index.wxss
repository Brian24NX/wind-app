/* components/customPicker/index.wxss */
.submitContainer {
  display: flex;
  justify-content: space-between;
  align-items: center;
  /* height: 90rpx; */
}

.submitContainer .oneBtn {
  width: 140rpx;
  text-align: left;
  padding: 30rpx 0 30rpx 30rpx;
  color: #999999;
  font-size: 30rpx;
}

.submitContainer .confirm {
  color: #071E5D;
  padding: 30rpx 30rpx 30rpx 0;
  text-align: right;
}

.onePicker {
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  padding: 0 30rpx;
}