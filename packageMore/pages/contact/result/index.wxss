/* packageMore/pages/contact/result/index.wxss */
page {
  background-color: #F0F1F5;
}

.contactNavbarContainer {
  background: url(https://ycyy-db-01.obs.cn-east-3.myhuaweicloud.com/test/contactTop.png) no-repeat;
  background-size: 100%;
  width: 100%;
  height: 302rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 2;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.contactNavbarContainer .backIcons {
  position: fixed;
  left: 0;
  z-index: 10;
  padding: 0 30rpx 0 10rpx;
  display: flex;
  align-items: center;
}

.contactNavbarContainer .message {
  width: 120rpx;
  height: 120rpx;
}

.contactNavbarContainer .contactTopTitle text:nth-of-type(1) {
  font-size: 48rpx;
  font-family: Antonio-Regular, Antonio;
  font-weight: 400;
  color: #071E5D;
}

.contactNavbarContainer .contactTopTitle text:nth-of-type(2) {
  font-size: 48rpx;
  font-family: Antonio-Bold, Antonio;
  font-weight: bold;
  color: #E20202;
}

.contactDetailContainer {
  padding: 302rpx 30rpx 40rpx;
}

.contactDetailContainer .contactLists .oneContact {
  padding: 0 28rpx;
  background-color: #ffffff;
  margin-bottom: 30rpx;
}

.contactDetailContainer .contactLists .oneContact .ones {
  display: flex;
  align-items: center;
  height: 168rpx;
}

.contactDetailContainer .contactLists .oneContact .ones image {
  width: 60rpx;
  height: 60rpx;
  margin-right: 40rpx;
}

.contactDetailContainer .contactLists .oneContact .ones .oneContent {
  width: 510rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  border-bottom: 1rpx solid #D9D9D9;
  color: #071E5D;
  font-size: 28rpx;
}

.contactDetailContainer .contactLists .oneContact .ones .oneContent view:nth-of-type(2) {
  margin-top: 5rpx;
}

.contactDetailContainer .contactLists .oneContact .ones .noLine {
  border-bottom: none;
}

.contactDetailContainer .noContact {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #6B7790;
  padding: 60rpx 100rpx 0;
  text-align: center;
  display: inline-block;
}