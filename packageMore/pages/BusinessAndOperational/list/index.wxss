/* packageMore/pages/BusinessAndOperational/index.wxss */
page {
  background-color: #F0F1F5;
}

.businessContainer .topContainer {
  background-image: url(https://ycyy-db-01.obs.cn-east-3.myhuaweicloud.com/test/yewu.png);
}

.businessContainer .topTitles {
  padding: 30rpx;
}

.businessContainer .topTitles .topBg {
  width: 100%;
  height: 210rpx;
}

.businessContainer .businessLists {
  width: 690rpx;
  margin: 0 auto 40rpx;
  background-color: #ffffff;
  padding: 1rpx 40rpx;
}

.businessList .oneBusiness {
  padding: 30rpx 0;
  border-bottom: 1rpx solid #EEEEEE;
}

.businessList .oneBusiness:nth-last-of-type(1) {
  border-bottom: none;
}

.businessList .oneBusiness .businessTitle {
  font-size: 28rpx;
  color: #04246A;
}

.businessList .oneBusiness .businessTime {
  color: #6B7790;
  font-size: 24rpx;
  margin-top: 6rpx;
}