/* packageMore/pages/sanctionCheck/list/index.wxss */
page {
  background-color: #F0F1F5;
}

.sanctionContainer {
  padding: 30rpx;
}

.sanctionContainer .topContainer {
  background-image: url(https://wind.cma-cgm.com/images/sanction.png);
}


.sanctionList {
  padding: 30rpx 48rpx;
  background-color: #ffffff;
  margin-top: 30rpx;
}

.sanctionList .oneSanction {
  padding: 30rpx 0;
  border-bottom: 1rpx solid #EEEEEE;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #071E5D;
}

.sanctionList .oneSanction:nth-last-of-type(1) {
  border-bottom: none;
}

.sanctionList .oneSanction image {
  width: 16rpx;
  height: 30rpx;
}

.reminds {
  font-size: 20rpx;
  color: #6B7790;
  text-align: center;
}

.reminds text {
  color: #071E5D;
}