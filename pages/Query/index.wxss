page {
  background-color: #F0F1F5;
}

.queryContainer {
  padding-bottom: 170rpx;
}

.queryContainer .menuLists {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 0 30rpx;
}

.queryContainer .menuLists .oneMenu {
  background-color: #ffffff;
  margin-top: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #071E5D;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  position: relative;
}

.queryContainer .menuLists .oneMenu .menuIcon {
  width: 96rpx;
  height: 96rpx;
  margin-bottom: 10rpx;
}

.queryContainer .menuLists .oneMenu .newIcon {
  position: absolute;
  top: 0;
  right: 0;
  width: 80rpx;
  height: 80rpx;
}

.queryContainer .menuLists .oneMenu view {
  padding: 0 20rpx;
  text-align: center;
}