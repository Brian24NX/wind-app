/* pages/LoginCopy/index.wxss */
.wrapper {
  width: 100%;
  min-height: 100vh;
  padding: 30rpx 105rpx 210rpx;
  background-color: white;
}

.header {
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.err{
  color: #c5002e;
  background: url(data:image/png;base64,) left 2px no-repeat;
  margin-top: 10px;
  padding:0 60rpx 0 39rpx;
  margin-bottom: 40rpx;
  margin-left: 60rpx;
}
.line{
  border-bottom: #cecece 1rpx solid;
  margin-right: 30rpx;
  margin-bottom: 40rpx;
}
.iconCMA {
  width: 155rpx;
  height: 95rpx;
}
.errImg{

}
.dropDownContainer{
  height: 46rpx;
  width: 180rpx;
  padding: 4rpx 8rpx;
  border-width: 1px;
  border-style: solid;
  border-color:rgb(118, 118, 118);
  overflow: visible;
  position: relative;
  font-size: 24rpx;
  color: #4C4C4C;
}
.arrow{
  position: absolute;
  right: 8rpx;
  top:10rpx;
  font-size: 28rpx;
}
.van-dropdown-menu__item {
  flex-direction: row;
  justify-content: left !important;
}

.mainContainer {
  width: 100%;
  height: 778rpx;
  margin-top: 30rpx;
}

.mainContainer .loginContainer {
  width: 100%;
  /*height: 700rpx;*/
  padding-bottom: 20rpx;
  font-family: Arial, Helvetica, sans-serif;
  background-color: #f0f1f5;
  margin-bottom: 110rpx;
}

.mainContainer .loginContainer .textSignIn {
  font-size: 1.7em;
  font-weight: normal;
  padding: 40rpx 60rpx;
  color: #184894;
}

.mainContainer .loginContainer .formArea {
  height: 500rpx;
  position: relative;
  margin:0 60rpx;
}

.titleLine {
  font-size: 0.8em;
  color: #4c4c4c;
  margin-bottom: 10rpx;
}

.inputContent {
  border: 1px solid #8d8d8d;
  background-color: white;
  font-size: 0.8em;
  margin-bottom: 40rpx;
  width: 400rpx;
}

.inputContent2 {
  border: 1px solid #8d8d8d;
  background-color: white;
  font-size: 0.8em;
  margin-bottom: 15rpx;
  width: 400rpx;
}

.forgotPassword {
  font-size: 0.7em;
  color: #2563d9;
  text-align: right;
  margin-bottom: 45rpx;
}

.signIn {
  display: inline-block;
  position: absolute;
  right: 0;
  border-radius: 0;
  background-color: #d81772;
  font-size: 0.65em;
  color: #fff;
  padding: 12rpx 24rpx;
}

.footNote {
  font-size: 0.7em;
  border-top: 1px solid #d7d7d9;
  text-align: center;
  padding: 17rpx 0 0 0;
  color: #4c4c4c;
  font-family: verdana, helvetica, arial, sans-serif
}