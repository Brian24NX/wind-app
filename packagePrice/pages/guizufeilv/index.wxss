/* packagePrice/pages/guizufeilv/index.wxss */
page {
  background-color: #F0F1F5;
}

.guizuContainer {
  padding: 30rpx;
}

.guizuContainer .topContainer {
  background-image: url(https://ycyy-db-01.obs.cn-east-3.myhuaweicloud.com/test/guizefeilv.png);
}

.guizuContainer .selectContainer {
  margin-top: 30rpx;
  background-color: #ffffff;
  padding: 40rpx 48rpx;
}

.guizuContainer .selectContainer .selectTitle {
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #04246A;
}

.guizuContainer .selects {
  width: 100%;
  height: 80rpx;
  border: 1rpx solid #dfdfdf;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 24rpx;
  color: #B9BFCA;
  font-size: 24rpx;
  margin-top: 20rpx;
}

.guizuContainer .selects view {
  width: 490rpx;
  display: inline-block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.guizuContainer .selects image {
  width: 32rpx;
  height: 32rpx;
}

.guizuContainer .guizuList {
  background-color: #ffffff;
  margin-top: 30rpx;
  padding: 20rpx 48rpx;
}

.guizuContainer .guizuList .titles {
  display: flex;
  font-size: 36rpx;
  font-family: Antonio-Bold;
  font-weight: bold;
  color: #04246A;
}

.guizuContainer .guizuList .titles .titleLine {
  width: 8rpx;
  height: 42rpx;
  background-color: #E20202;
  margin-right: 16rpx;
}

.guizuContainer .guizuList .van-cell {
  padding-left: 0;
  padding-right: 0;
}

.guizuContainer .guizuList .items {
  padding: 0 0 20rpx;
}

.guizuContainer .guizuList .fileList .oneFile {
  border: 1rpx solid #CFD2DA;
  padding: 20rpx;
  display: flex;
  margin-top: 20rpx;
}

.guizuContainer .guizuList .fileList .oneFile:nth-of-type(1) {
  margin-top: 0;
}

.guizuContainer .guizuList .fileList .oneFile .infoIcon {
  width: 96rpx;
  height: 96rpx;
  margin-right: 10rpx;
}

.guizuContainer .guizuList .fileList .oneFile .templateContent {
  width: 390rpx;
  font-weight: 400;
  color: #6B7790;
  font-size: 28rpx;
}

.guizuContainer .guizuList .fileList .oneFile .templateContent .templateName {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #071E5D;
}

.guizuContainer .guizuList .fileList .oneFile .preview {
  width: 56rpx;
  text-align: right;
  padding-top: 20rpx;
}

.guizuContainer .guizuList .fileList .oneFile .preview image {
  width: 40rpx;
  height: 40rpx;
}