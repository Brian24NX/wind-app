/* packageBooking/pages/Detail/index.wxss */
page {
  background-color: #F0F1F5;
  padding: 30rpx 30rpx env(safe-area-inset-bottom);
}

.oneDetails {
  background-color: #ffffff;
  padding: 40rpx 30rpx;
  margin-bottom: 30rpx;
}

.detailTops {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-bottom: 38rpx;
}

.detailTops .detailTopLeft {
  display: flex;
  font-size: 48rpx;
  font-family: Antonio-Regular, Antonio;
  font-weight: 400;
  color: #071E5D;
}

.detailTops .detailTopLeft image {
  width: 70rpx;
  height: 70rpx;
  margin-right: 20rpx;
}

.detailTops .editIcon {
  width: 70rpx;
  height: 70rpx;
}

.routeContainer {
  background: linear-gradient(180deg, #EEF1F6 0%, #F3F6FC 100%);
  padding: 18rpx;
}

.routeContainer .stepLists {
  padding-left: 50rpx;
}

.routeContainer .stepLists .oneStep {
  padding-bottom: 10rpx;
  position: relative;
}

.routeContainer .stepLists .oneStep:nth-last-of-type(1) {
  padding-bottom: 0;
}

.routeContainer .stepLists .oneStep .leftLines {
  position: absolute;
  left: -50rpx;
  height: 100%;
  top: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 24rpx;
}

.routeContainer .stepLists .oneStep .leftLines image {
  width: 24rpx;
  height: 30rpx;
}

.routeContainer .stepLists .oneStep .leftLines .leftLineDto {
  background-color: #E20202;
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
}

.routeContainer .stepLists .oneStep .leftLines .leftLine {
  width: 0;
  height: calc(100% - 16rpx);
  border-left: 1rpx solid #9AA6C4;
}

.routeContainer .stepLists .oneStep .stepContent {
  font-size: 52rpx;
  font-family: Antonio-Regular, Antonio;
  font-weight: 400;
  color: #071E5D;
}

.portTimeContainer {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 22rpx 40rpx 22rpx 10rpx;
  border-bottom: 1rpx solid #FFFFFF;
  border-top: 1rpx solid #FFFFFF;
  margin-top: 22rpx;
}

.portTimeContainer .portTimes {
  color: #04246A;
  font-size: 24rpx;
}

.portTimeContainer .portTimes .portTime {
  font-size: 40rpx;
  font-family: Antonio-Bold, Antonio;
  font-weight: bold;
  color: #04246A;
}

.cutTimeContainers {
  border-bottom: 1rpx solid #F4F4F4;
}

.cutTimeContainers .cutTimeContainer {
  background: linear-gradient(180deg, #EEF1F6 0%, #F3F6FC 100%);
}

.cutTimeContainers .cutTimeContainer .oneCutTime {
  padding: 20rpx 0;
  border-bottom: 1rpx solid #FFFFFF;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  font-weight: 500;
  font-size: 24rpx;
  color: #6B7790;
}

.cutTimeContainers .cutTimeContainer .oneCutTime:nth-last-of-type(1) {
  border-bottom: none;
}

.cutTimeContainers .cutTimeContainer .oneCutTime view:nth-last-of-type(1) {
  color: #071E5D;
}

.oneCargo {
  margin-bottom: 30rpx;
  background: linear-gradient(180deg, #EEF1F6 0%, #F3F6FC 100%);
}

.oneCargo:nth-last-of-type(1) {
  margin-bottom: 0;
}

.oneCargo .cargoTitle {
  padding: 16rpx 30rpx;
  font-size: 36rpx;
  font-family: Antonio-Regular, Antonio;
  font-weight: 400;
  color: #04246A;
  background-color: #DBDFE8;
}

.oneCargo .cargoValue {
  padding: 24rpx 18rpx;
  font-size: 32rpx;
  color: #6B7790;
}

.paymentTitle {
  color: #04246A;
  font-size: 32rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.paymentTitle image {
  width: 40rpx;
  height: 40rpx;
}

.paymentType {
  display: flex;
  margin-top: 20rpx;
  color: #04246A;
  font-size: 32rpx;
}

.paymentType .onePaymentType {
  display: flex;
  align-items: center;
  width: 40%;
}

.paymentType .onePaymentType .paymentCheck {
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  border: 1rpx solid #04246A;
  margin-right: 12rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.paymentType .onePaymentType .paymentCheck .checked {
  width: 22rpx;
  height: 22rpx;
  border-radius: 50%;
  background-color: #04246A;
}

.inputContainers {
  border: 1rpx solid #DFDFDF;
  width: 100%;
  height: 80rpx;
}

.mar {
  margin: 40rpx 0 12rpx;
}

.inputContainers input {
  height: 100%;
  padding: 0 20rpx;
}

.textareaContainers {
  width: 100%;
  height: 200rpx;
  border: 1rpx solid #DFDFDF;
}

.textareaContainers textarea {
  height: 100%;
  padding: 20rpx;
}

.sureBtn {
  width: 100%;
  height: 88rpx;
  background-color: #2D75FF;
  border-radius: 0;
  color: #ffffff;
  font-size: 36rpx;
  font-weight: 500;
}

.additionContainer {
  padding: 10rpx 20rpx 30rpx;
  background: linear-gradient(180deg, #EEF1F6 0%, #F3F6FC 100%);
}

.additionContainer .oneAddition {
  padding: 22rpx 0 40rpx;
  border-bottom: 1rpx solid #EEEEEE;
}

.additionContainer .oneAddition .vasTitle {
  font-weight: 600;
  color: #04246A;
  font-size: 28rpx;
}

.additionContainer .oneAddition .vasBottom {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  margin-top: 16rpx;
}

.additionContainer .oneAddition .vasBottom .vasBottomLeft {
  font-size: 24rpx;
  color: #6B7790;
  width: 380rpx;
}

.additionContainer .oneAddition .vasBottom .vasBottomRight {
  display: flex;
  align-items: flex-end;
  font-family: Antonio-Regular, Antonio;
  font-weight: 400;
}

.additionContainer .oneAddition .vasBottom .vasBottomRight .totalAmount {
  font-size: 48rpx;
  color: #04246A;
  margin-right: 10rpx;
  line-height: 48rpx;
}

.additionContainer .oneAddition .vasBottom .vasBottomRight .amountDesc {
  font-size: 24rpx;
  color: #071E5D;
  line-height: 24rpx;
}

.additionContainer .oneAddition .vasBottom .vasBottomRight .amountDesc view:nth-of-type(1) {
  margin-bottom: 10rpx;
}