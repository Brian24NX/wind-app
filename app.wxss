/* 引入公共样式 */
@import './assets/styles/common.wxss';

/**app.wxss**/
page{
  background: #f5f5f5;
  font-family: "微软雅黑", "华文细黑", Arial, Helvetica, sans-serif;
  height: 100%;
  width: 100%;
  position: relative;
  z-index: 99999;
  background: #fff;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
