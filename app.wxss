/* 引入公共样式 */
@import './assets/styles/common.wxss';

/**app.wxss**/
page{
  background: #f5f5f5;
  font-family: "微软雅黑", "华文细黑", Arial, Helvetica, sans-serif;
  height: 100%;
  width: 100%;
  position: relative;
  z-index: 99999;
  background: #fff;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.blue{
   color:#04246A;
}
.red{
   color:#E20101;
}
.blue_secondary{
   color:#3E64B8;
}
.blur_link{
   color:#235AF6;
}
.blue_hover{
   color:#031745;
}
.red_hover{
   color:#B20101;
}
