/* packageDashboard/pages/seaRewardFAQ/index.wxss */
page {
  background-color: #F0F1F5;
}

.resultContainers {
  width: 100%;
  min-height: 100vh;
  padding: 30rpx 30rpx 210rpx;
  background-color: #f0f1f5;
}


.resultContainers .rateContainer {
  display: flex;
  flex-direction: column;
  padding: 30rpx 30rpx 0rpx 30rpx;
  margin-top: -20rpx;
  position: relative;
}

.resultContainers .rateContainer .rateItem {
  background-color: rgba(255, 255, 255, 1.000000);
  width: 690rpx;
  margin-left: -30rpx;
  margin-bottom: 25rpx;
  flex-direction: row;
  display: flex;
  justify-content: flex-center;
  padding: 28rpx 30rpx 28rpx 30rpx;
  position: relative;
}

.resultContainers .rateContainer .rateItem .titleLine {
  background-color: rgba(226, 2, 2, 1.000000);
  width: 8rpx;
  height: 42rpx;
  display: flex;
  flex-direction: column;
  margin: 4rpx 0 4rpx 0;
}

.resultContainers .rateContainer .rateItem .titleText {
  width: 120rpx;
  overflow-wrap: break-word;
  color: rgba(7, 30, 93, 1);
  font-size: 32rpx;
  letter-spacing: -1rpx;
  font-family: Antonio-Regular;
  font-weight: normal;
  text-align: left;
  white-space: nowrap;
  line-height: 46rpx;
  margin: 0 0 0 20rpx;
}

.resultContainers .rateContainer .rateItem .iconAnchor {
  width: 50rpx;
  height: 50rpx;
  margin-left: 380rpx;
}

.resultContainers .rateContainer .rateItem .editIcon {
  width: 32rpx;
  height: 32rpx;
  margin: 10rpx 0 8rpx 20rpx;
  margin-left: 440rpx;
}

.editIcon2 {
  transform: rotate(180deg);
}

.hiddenContainer {
  background-color: rgba(255, 255, 255, 1.000000);
  align-self: center;
  width: 690rpx;
  display: flex;
  flex-direction: column;
  padding: 25rpx 30rpx 0rpx 30rpx;
  margin-bottom: 25rpx;
  margin-top: -25rpx;
  margin-left: -5rpx;
  /* border-top: 1rpx solid rgba(240, 241, 245, 1);  */
}
.hiddenContainer::before,
.hiddenContainer::after {
  content: "";
  display: block;
  width:calc(100% - 60rpx);
  margin: 0 30rpx;
  height: 1rpx;
  position: absolute;
  left: 0;
}

.hiddenContainer::before {
  background: #EEEEEE;
  top: 0;
}

.hiddenContainer {
  position: relative;
}

